<?xml version="1.0" encoding="utf-8"?>
<s:SparkButtonSkin xmlns:fx="http://ns.adobe.com/mxml/2009" 
             xmlns:s="library://ns.adobe.com/flex/spark" 
             xmlns:fb="http://ns.adobe.com/flashbuilder/2009"
             minWidth="21" minHeight="21" 
             alpha.disabled="0.5" xmlns:local="*">
     
    <fx:Metadata>
        <![CDATA[ 
        /** 
         * @copy spark.skins.spark.ApplicationSkin#hostComponent
         */
        [HostComponent("spark.components.Button")]
        ]]>
    </fx:Metadata>
    
    <fx:Script fb:purpose="styling">
        <![CDATA[         
                    
        ]]>        
    </fx:Script>
        
    <!-- states -->
    <s:states>
        <s:State name="up" />
        <s:State name="over" />
        <s:State name="down" />
        <s:State name="disabled" />
    </s:states>
	
	<local:SkyLeftButtonDownSkin includeIn="down, over" />
	<local:SkyLeftButtonUpSkin excludeFrom="down, over" />
	<s:RichText id="labelDisplay" color="#ffffff" color.down="#0099FF" color.over="#0099FF" fontSize="37"
				kerning="on" tabStops="S0 S50 S100 S150" textAlign="center"
				left.disabled="1" right.disabled="6" top.disabled="20" bottom.disabled="22"
				left.down="0" right.down="0" top.down="20" bottom.down="0"
				left.over="0" right.over="0" top.over="20" bottom.over="0"
				left.up="0" right.up="0" top.up="20" bottom.up="0"/>
    
</s:SparkButtonSkin>
